curl <span class="pl-k">-</span><span class="pl-c1">X</span> <span class="pl-c1">POST</span> <span class="pl-k">--</span>data <span class="pl-s"><span class="pl-pds">'</span>{"jsonrpc":"2.0", "method":"eth_submitWork", "params":["0x0000000000000001", "0x1234567890abcdef1234567890abcdef1234567890abcdef1234567890abcdef", "0xD1GE5700000000000000000000000000D1GE5700000000000000000000000000"],"id":73}<span class="pl-pds">'</span></span>

